                        $(".biao2tiss").html(biao2ti)
                        $(".neirongss").html(neirong)
                        $(".xianshiqy").fadeIn()
                    }

                    $("#ulul  li").click(function () {
                        index = $(this).index();
                        huoqu(index)
                    });
                    $(".zuo").click(function () {
                        if (index >= 0) {
                            index -= 1;
                            huoqu(index)

                        }
                        if (index == -1) {
